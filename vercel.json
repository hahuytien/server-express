{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/screenshot.js": {
            "maxDuration": 60,
            "runtime": "now-node@1.6.1"
        }
    }
}